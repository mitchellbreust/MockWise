{
  "name": "interviewpracapp",
  "version": "1.0.0",
  "description": "Interview practice application with OpenAI integration",
  "main": "src/server/index.ts",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/server/index.js",
    "dev": "ts-node-dev --respawn src/server/index.ts",
    "build": "npm run build:server && npm run build:client",
    "test": "ts-node src/test/testClient.ts",
    "build-client": "tsc src/client/app.ts --outDir src/client/dist",
    "build:server": "tsc -p tsconfig.json",
    "build:client": "cd src/client/frontend && npm run build",
    "deploy": "npm run build && eb deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "assemblyai": "^4.9.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "mammoth": "^1.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.0.0",
    "pdf-parse": "^1.1.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "ws": "^8.16.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.16",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
